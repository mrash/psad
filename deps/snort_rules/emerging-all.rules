alert tcp any any -> any any (msg:"ET TROJAN perlb0t/w0rmb0t Response (Case 3)"; flowbits:isset,is_proto_irc; content:"|3A 02|"; content:"|02|"; within: 32; pcre:"/\x3A\x02(Alvo dos Pacotes|Conectando-se em|Mdia de envio|Tempo.*|Total .*)\x02/i"; classtype: trojan-activity; sid:2006912; rev:4;)


